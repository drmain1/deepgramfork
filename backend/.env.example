# Deepgram API Key for speech-to-text
deepgram_api_key=your_deepgram_api_key_here

# Speechmatics API Key for multilingual support
SPEECHMATICS_API_KEY=your_speechmatics_api_key_here

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1
AWS_S3_BUCKET_NAME=your_s3_bucket_name

# Auth0 Configuration
AUTH0_DOMAIN=your_auth0_domain
AUTH0_AUDIENCE=your_auth0_audience
AUTH0_MGMT_CLIENT_ID=your_auth0_mgmt_client_id
AUTH0_MGMT_CLIENT_SECRET=your_auth0_mgmt_client_secret

# Google Cloud Platform Configuration (for Gemini Pro)
GCP_PROJECT_ID=your_gcp_project_id
GCP_LOCATION=us-central1
# Option 1: Path to service account JSON file
GCP_CREDENTIALS_PATH=/path/to/your/service-account-key.json
# Option 2: Or set GOOGLE_APPLICATION_CREDENTIALS environment variable
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account-key.json